<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:background="@drawable/grant_side"
    android:layout_height="fill_parent">

    <TextView
        android:text="@string/EngTitle"
        android:id="@+id/EngTitle"
        style="@style/Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="35dp"
        android:layout_centerHorizontal="true"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/linearLayout"
        android:layout_below="@+id/EngTitle"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="20dp">


        <Button
            android:layout_width="105dp"
            android:layout_height="85dp"
            android:layout_weight="1"
            android:text="Chernoff\nHall"
            android:id="@+id/Chernoff"
            style="@style/Type2"
            android:background="@drawable/button_building"
            android:layout_margin="7dp" />


        <Button
            android:layout_width="105dp"
            android:layout_height="85dp"
            android:layout_weight="1"
            android:text="Clark \nHall"
            android:id="@+id/Clark"
            style="@style/Type2"
            android:background="@drawable/button_building"
            android:layout_margin="7dp" />

        <Button
            android:layout_width="105dp"
            android:layout_height="85dp"
            android:layout_weight="1"
            android:text="Dupuis\nHall"
            android:id="@+id/Dupuis"
            style="@style/Type2"
            android:background="@drawable/button_building"
            android:layout_margin="7dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/linearLayout2"
        android:layout_below="@+id/linearLayout"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="5dp">

        <Button
            android:layout_width="105dp"
            android:layout_height="85dp"
            android:layout_weight="1"
            android:text="Ellis\nHall"
            style="@style/Type2"
            android:background="@drawable/button_building"
            android:id="@+id/Ellis"
            android:layout_margin="7dp" />

        <Button
            android:layout_width="105dp"
            android:layout_height="85dp"
            android:layout_weight="1"
            android:text="Goodwin \nHall"
            style="@style/Type2"
            android:background="@drawable/button_building"
            android:id="@+id/Goodwin"
            android:layout_margin="7dp" />

        <Button
            android:layout_width="105dp"
            android:layout_height="85dp"
            android:layout_weight="1"
            android:text="ILC\n"
            style="@style/Type2"
            android:background="@drawable/button_building"
            android:id="@+id/ILC"
            android:layout_margin="7dp" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/linearLayout3"
        android:layout_below="@+id/linearLayout2"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp">

        <Button
            android:layout_width="105dp"
            android:layout_height="85dp"
            android:layout_weight="1"
            android:text="Innovation\nCommons"
            style="@style/Type2"
            android:background="@drawable/button_building"
            android:id="@+id/IC"
            android:layout_margin="7dp" />

        <Button
            android:layout_width="105dp"
            android:layout_height="85dp"
            android:layout_weight="1"
            android:text="Jeffery\nHall"
            android:id="@+id/Jeffery"
            style="@style/Type2"
            android:background="@drawable/button_building"
            android:layout_margin="7dp" />

        <Button
            android:layout_width="105dp"
            android:layout_height="85dp"
            android:layout_weight="1"
            android:text="Mclaughlin\nHall"
            android:id="@+id/Mclaughlin"
            style="@style/Type2"
            android:background="@drawable/button_building"
            android:layout_margin="7dp" />



    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/linearLayout4"
        android:layout_below="@+id/linearLayout3"
        android:layout_alignParentStart="true">

        <Button
            android:layout_width="105dp"
            android:layout_height="85dp"
            android:layout_weight="1"
            android:text="Miller\nHall"
            style="@style/Type2"
            android:background="@drawable/button_building"
            android:id="@+id/Miller"
            android:layout_margin="7dp" />


        <Button
            android:layout_width="105dp"
            android:layout_height="85dp"
            android:layout_weight="1"
            android:text="Stirling\nHall"
            style="@style/Type2"
            android:background="@drawable/button_building"
            android:id="@+id/Stirling"
            android:layout_margin="7dp" />

        <Button
            android:layout_width="105dp"
            android:layout_height="85dp"
            android:layout_weight="1"
            android:text="Walter\nLight"
            android:id="@+id/Walter"
            style="@style/Type2"
            android:background="@drawable/button_building"
            android:layout_margin="7dp" />



    </LinearLayout>



</RelativeLayout>