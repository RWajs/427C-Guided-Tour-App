<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:background="@drawable/limestone_dark2"
    android:layout_height="fill_parent"
    android:fillViewport="false"
    android:id="@+id/ScrollView"
  >

    <LinearLayout
        android:layout_width="fill_parent"
        android:orientation="vertical"
        android:id="@+id/linearLayout1"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/TitleContactsPage"
            android:id="@+id/TitleContact"
            android:layout_gravity="center_horizontal"
            style="@style/TitleContactsPage"/>
        <Space
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_gravity="center"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/GenInfo"
            android:id="@+id/GenInfo"
            style="@style/SubTitleContactsPage" />

        <Space
            android:layout_width="15dp"
            android:layout_height="5dp"
            android:layout_gravity="center"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/C1"
            android:id="@+id/C1"
            style="@style/PersonContactsPage" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Info1"
            android:id="@+id/Info1"
            style="@style/InformationContactsPage"
            android:autoLink="email"
            android:textColorLink="@android:color/holo_blue_light"
            />

        <Space
            android:layout_width="15dp"
            android:layout_height="10dp"
            android:layout_gravity="center"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/C2"
            android:id="@+id/C2"
            style="@style/PersonContactsPage" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Info2"
            android:id="@+id/Info2"
            style="@style/InformationContactsPage"
            android:autoLink="email"
            android:textColorLink="@android:color/holo_blue_light"/>

        <Space
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_gravity="center"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/FirstYO"
            android:id="@+id/FirstYO"
            style="@style/SubTitleContactsPage" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/C3"
            android:id="@+id/C3"
            style="@style/PersonContactsPage" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Info3"
            android:id="@+id/Info3"
            style="@style/InformationContactsPage"
            android:autoLink="email"
            android:textColorLink="@android:color/holo_blue_light"/>

        <Space
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_gravity="center"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/VisandInfoCent"
            android:id="@+id/VisandInfoCent"
            style="@style/SubTitleContactsPage" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/C4"
            android:id="@+id/C4"
            style="@style/PersonContactsPage"
            android:autoLink="email"
            android:textColorLink="@android:color/holo_blue_light"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Info4"
            android:id="@+id/Info4"
            style="@style/InformationContactsPage"
            android:autoLink="phone"
            android:textColorLink="@android:color/holo_blue_light"/>

        <Space
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_gravity="center"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DirFirstYO"
            android:id="@+id/DirFirstYO"
            style="@style/SubTitleContactsPage" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/C5"
            android:id="@+id/C5"
            style="@style/PersonContactsPage" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Info5"
            android:id="@+id/Info5"
            style="@style/InformationContactsPage"
            android:autoLink="all"
            android:textColorLink="@android:color/holo_blue_light"/>

        <Space
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_gravity="center"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/EngQandInfo"
            android:id="@+id/EngQandInfo"
            style="@style/SubTitleContactsPage" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/C6"
            android:id="@+id/C6"
            style="@style/PersonContactsPage" android:autoLink="email"
            android:textColorLink="@android:color/holo_blue_light"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Info6"
            android:id="@+id/Info6"
            style="@style/InformationContactsPage"
            android:autoLink="phone"
            android:textColorLink="@android:color/holo_blue_light"/>

        <Space
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_gravity="center"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Useful"
            android:id="@+id/Useful"
            style="@style/SubTitleContactsPage" />

        <Space
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_gravity="center"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/StuBlog"
            android:id="@+id/StuBlog"
            android:textColorLink="#D5D5D5"
            style="@style/PersonContactsPage" />

        <Space
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_gravity="center"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Social"
            android:id="@+id/Social"
            style="@style/SubTitleContactsPage" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Info11"
            android:id="@+id/Info11"
            style="@style/InformationContactsPage"
            android:textColorLink="@android:color/holo_blue_light"/>

    </LinearLayout>

</ScrollView>
